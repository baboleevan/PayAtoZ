         url('MyriadPro-Regular.woff') format('woff'),
         url('MyriadPro-Regular.ttf') format('truetype'),
         url('MyriadPro-Regular.svg#MyriadPro') format('svg');
		 font-weight:normal;
		 font-style:normal;
	 
}

